---
layout: null
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
    <title type="text" xml:lang="en">{{ site.title | xml_escape }}</title>
        <subtitle type="html">Web design including: UI, UX, HTML, CSS, JavaScript and Node</subtitle>
    <link type="application/atom+xml" href="{{site.url}}/feed.xml" rel="self" />
    <link href="{{site.url}}" rel="alternate" />
    <link href="https://vincentp.superfeedr.com/" rel="hub" />
    <updated>{{site.time  | date_to_xmlschema}}</updated>
    <id>{{site.url}}/</id>
    <author>
        <name>{{site.author.name}}</name>
         <uri>{{site.url}}</uri>
        <email>{{site.author.email}}</email>
    </author>
    <rights>Copyright (c) 2013-2017, {{site.author.name}}; all rights reserved.</rights>
    {% for post in site.posts limit:20 %}
        <entry>
            <title>{{ post.title | xml_escape }}</title>
            <link href="{{site.url}}{{ post.url }}"/>
            <id>{{site.url}}{{post.id}}</id>
            <content type="html">{{ post.content | xml_escape}}</content>
            <updated>{{post.modified  | date_to_xmlschema}}</updated>
        </entry>
    {% endfor %}
</feed>
