{
    "rules": {
        "no-todo": true,
        "no-exclamation-question-mark": true,
        "no-empty-section": true,
        "date-weekday-mismatch": {
            "lang": "en"
        },
        "en-max-word-count": {
            "max" : 50
        },
         "alex": {
            "allow": []
        },
        "common-misspellings": {
            // Misspellings to be ignored (case-insensitive)
            "ignore": []
        },
         "rousseau": {
            "showLevels": ["suggestion", "warning", "error"],
            "ignoreTypes": ["sentence:uppercase"]
        },
        "no-start-duplicated-conjunction": {
            "interval" : 2 // interval of sentences
        },
        "max-number-of-lines": {
            "max" : 300
        },
        "max-comma": {
            "max" : 3
        },
        "unexpanded-acronym" : {
            // AB is ignore, ABC is recognized.
            "min_acronym_len" : 3,
            // GREEEEEEN is ignore
            "max_acronym_len" : 5,
            // OSS is ignore
            "ignore_acronyms" : ["OSS", "HTML", "SASS", "CSS", "HTTP", "HTTPS", "LESS", "FTP", "URL", "GOV"]
        },
        "terminology": {
              "defaultTerms": false,
              // Syntax elements to skip. Overrides the default
              "skip": ["Blockquote"],
              "terms": "/Users/vincentpickering/Sites/personal/vincentp/_terms.json"
            }
    }
}


