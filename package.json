{
  "name": "vincentp.me",
  "version": "8.0.0",
  "description": "personal website",
  "main": "https://vincentp.me",
  "author": "Vincent Pickering",
  "homepage": "https://github.com/vipickering/vincentp#readme",
  "license": "ISC",
  "private": true,
  "scripts": {
    "start": "jekyll serve",
    "postcss": "postcss _css/application.css --config postcss.config.js --output _includes/application.min.css",
    "test": "pa11y-ci --sitemap https://vincentp.me/sitemap.xml --sitemap-find https://vincentp.me --sitemap-replace http://localhost:4000 --sitemap-exclude https://vincentp.me/feed.json",
    "uglify": "uglifyjs _js/service-worker.js -o service-worker.min.js -c",
    "dev": "postcss _css/application.css --config postcss.config.js --output _includes/application.min.css --watch & jekyll serve --drafts --watch --future",
    "production": "jekyll serve JEKYLL_ENV=production"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vipickering/vincentp.git"
  },
  "bugs": {
    "url": "https://github.com/vipickering/vincentp/issues"
  },
  "devDependencies": {
    "css-mqpacker": "^6.0.1",
    "cssnano": "^3.10.0",
    "pa11y": "^4.13.1",
    "pa11y-ci": "^1.3.0",
    "postcss": "^6.0.13",
    "postcss-cli": "^4.1.1",
    "postcss-copy": "^7.1.0",
    "postcss-css-variables": "^0.8.0",
    "postcss-cssnext": "^3.0.2",
    "postcss-import": "^11.0.0",
    "postcss-reporter": "^5.0.0",
    "stylelint": "^8.2.0",
    "uglify-js": "^3.1.7"
  },
  "dependencies": {}
}
